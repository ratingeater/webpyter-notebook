# Backend Kernel URL - set this to your Python kernel server
VITE_BACKEND_KERNEL_URL=http://localhost:5000

# Default kernel mode (optional): backend | pyodide
# VITE_DEFAULT_KERNEL_MODE=pyodide

# Collaboration (optional)
# WebSocket base URL (Cloudflare Worker), the client connects to: ${VITE_COLLAB_WS_URL}/{notebookId}
# VITE_COLLAB_WS_URL=ws://localhost:8787/ws
# Optional shared token sent as query param (?token=...) when connecting
# VITE_COLLAB_TOKEN=dev-token
# Connection timeout (ms) before falling back to local-only mode
# VITE_COLLAB_CONNECT_TIMEOUT_MS=2000
